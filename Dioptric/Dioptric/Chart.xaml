<Window x:Class="Dioptric.Chart"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dioptric"
        mc:Ignorable="d"
        Title="Chart" Height="768" Width="1280" Loaded="Window_Loaded">
    <Grid Background="LightGreen">
        <Canvas Name="canGraph" Background="White"
      Width="960" Height="600"
      VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="年龄（岁）" FontSize="20" VerticalAlignment="Top" Canvas.Left="850" Canvas.Top="565"/>
            <TextBlock Canvas.Left="10" TextWrapping="Wrap" Text="视力" Canvas.Top="10" FontSize="20" Width="20"/>
        </Canvas>
    </Grid>
</Window>
